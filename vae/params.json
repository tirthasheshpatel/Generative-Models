{
    "input_dims": [28, 28, 1],
    "latent_dims": 3,
    "batch_size": 100,
    "learning_rate": 0.001,
    "epochs": 10,
    "optimizer": "Adam",
    "loss": "normal_vlb",

    "encoder": {
        "name": "encoder",
        "latent_dims": 3,
        "channels": [32, 64],
        "kernel_widths": [3, 3],
        "strides": [2, 2],
        "hidden_activation": "relu",
        "output_activation": "linear"
    },

    "decoder": {
        "name": "decoder",
        "latent_dims": 3,
        "initial_dense_dims": 1568,
        "initial_activation": "relu",
        "starting_target_shape": [7, 7, 32],
        "channels": [64, 32],
        "kernel_widths": [3, 3],
        "strides": [2, 2],
        "hidden_activation": "relu"
    }
}